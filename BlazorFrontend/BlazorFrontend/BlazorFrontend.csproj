<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Blazored.FluentValidation" Version="2.2.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="9.0.0" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="9.0.0" PrivateAssets="all" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="API">
      <HintPath>..\..\backend\API\obj\Debug\net9.0\ref\API.dll</HintPath>
    </Reference>
    <Reference Include="Models">
      <HintPath>..\..\backend\API\bin\Debug\net9.0\Models.dll</HintPath>
    </Reference>
    <Reference Include="Services">
      <HintPath>..\..\backend\API\bin\Debug\net9.0\Services.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
